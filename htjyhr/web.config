            <add name="*.php" path="*.php" verb="*" modules="CgiModule" scriptProcessor="C:\phpStudy\PHPTutorial\php\php-7.0.12-nts\php-cgi.exe" resourceType="Unspecified" requireAccess="Script" /> 
        </handlers> -->
    </system.webServer> 
</configuration> 