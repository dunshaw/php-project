<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
     	<directoryBrowse enabled="true" />
        <httpErrors errorMode="Detailed" />
        <rewrite>
            <rules>
                <rule name="已导入的规则 1">
                    <match url="^admin(.*)$" />
                   
                    <action type="Rewrite" url="index.php?route=admin{R:1}" appendQueryString="true" />
                </rule>
              
                <rule name="已导入的规则 3">
                    <match url="^(.*)$" />
                    <action type="Rewrite" url="index.php?route=front/{R:1}" appendQueryString="true" />
                </rule>
             
            </rules>
        </rewrite> 
        <handlers>		  
            <add name="*.php" path="*.php" verb="*" modules="CgiModule" scriptProcessor="C:\phpStudy\PHPTutorial\php\php-7.0.12-nts\php-cgi.exe" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
    </system.webServer> 
</configuration> 